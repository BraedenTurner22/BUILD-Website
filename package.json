{
	"name": "build-umass-website",
	"version": "3.0.1",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "yarn prettier --write . && next build && next export",
		"start": "next start",
		"serve": "firebase serve",
		"format": "yarn prettier --write ./pages && yarn prettier --write ./styles && yarn prettier --write ./components"
	},
	"dependencies": {
		"@fortawesome/fontawesome-free": "^5.12.1",
		"@fortawesome/fontawesome-svg-core": "^1.2.27",
		"@fortawesome/free-brands-svg-icons": "^5.12.1",
		"@fortawesome/free-regular-svg-icons": "^5.12.1",
		"@fortawesome/free-solid-svg-icons": "^5.12.1",
		"@fortawesome/react-fontawesome": "^0.1.9",
		"bootstrap": "^4.4.1",
		"firebase-tools": "^7.15.1",
		"jquery": "^1.9.1",
		"next": "9.3.1",
		"popper.js": "^1.16.0",
		"react": "16.13.1",
		"react-bootstrap": "^1.0.0-beta.17",
		"react-dom": "16.13.1",
		"react-particles-js": "^2.7.1",
		"sass": "^1.26.3"
	},
	"devDependencies": {
		"husky": "^4.2.3",
		"prettier": "2.0.1",
		"pretty-quick": "^2.0.1"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"linters": {
			"./**/*.{js,css}": [
				"prettier --write",
				"git add"
			]
		}
	}
}
